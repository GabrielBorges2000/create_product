<table class="table table-striped table-hover table-bordered">
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Produto</th>
      <th scope="col">Quantidade</th>
      <th scope="col">Valor Unit√°rio R$</th>
    </tr>
  </thead>
  <tbody>
    <% table.forEach((item, index)=> { %>
      <tr>
        <th scope="row">
          <%= index %>
        </th>
        <td>
          <%= item.productName %>
        </td>
        <td>
          <%= item.stock %>
        </td>
        <td>
          <%= item.value %>
        </td>
      </tr>
      <% }); %>

